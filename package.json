{
    "name": "houselist-server",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "test:e2e": "jest --runInBand --forceExit --selectProjects e2e",
        "test": "jest --selectProjects unit",
        "test:all": "jest --runInBand --forceExit",
        "start": "serverless offline"
    },
    "jest": {
        "projects": [
            {
                "displayName": "unit",
                "testMatch": [
                    "<rootDir>/test/**/*.test.js"
                ]
            },
            {
                "displayName": "e2e",
                "testMatch": [
                    "<rootDir>/e2e/**/*.test.js"
                ]
            }
        ]
    },
    "dependencies": {
        "@aws-sdk/client-rds": "^3.45.0",
        "@aws-sdk/client-s3": "^3.45.0",
        "@aws-sdk/s3-request-presigner": "^3.45.0",
        "axios": "^0.25.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.1",
        "date-fns": "^2.27.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "firebase-admin": "^10.0.2",
        "handlebars": "^4.7.7",
        "jest": "^27.4.5",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.15",
        "pg": "^8.7.1",
        "serverless-http": "^2.7.0",
        "sharp": "^0.29.3",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.5",
        "@types/jest": "^27.0.3",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "mock-knex": "^0.4.10",
        "prettier": "^2.5.1",
        "serverless-dotenv-plugin": "^3.11.1",
        "serverless-offline": "^8.3.1",
        "sinon": "^12.0.1",
        "supertest": "^6.1.6"
    }
}
